{
  "name": "vue-lib-monorepo",
  "version": "0.0.0",
  "description": "A monorepo template for developing vue libraries",
  "author": "Thorsten LÃ¼nborg <thorsten@linusb.org>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "playground",
    "docs"
  ],
  "main": "index.js",
  "scripts": {
    "start": "lerna run start --stream --parallel --scope '{lib}'",
    "test": "lerna run test --",
    "lint": "lerna run lint -- --fix",
    "build": "lerna run build",
    "play": "cd playground && yarn start",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "changeset": "changeset",
    "release": "changeset publish",
    "version:canary": "changeset version --snapshot canary",
    "release:canary": "changeset publish --tag canary"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.2.7",
    "@changesets/cli": "^2.10.3",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-beta.13",
    "autoprefixer": "^10.1.0",
    "cypress": "^5.2.0",
    "eslint-plugin-vue": "^7.3.0",
    "lerna": "^3.15.0",
    "postcss": "^8.2.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "tsdx": "^0.14.0",
    "typescript": "^3.9.7",
    "vue": "^3.0.4",
    "vue-jest": "^5.0.0-alpha.7"
  }
}
